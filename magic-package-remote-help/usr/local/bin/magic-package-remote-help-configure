#!/bin/bash

gsettings set org.gnome.Vino enabled true
gsettings set org.gnome.Vino view-only false
gsettings set org.gnome.Vino vnc-password '$PASSWORD_ADMIN_64'
gsettings set org.gnome.Vino authentication-methods "['vnc']"
gsettings set org.gnome.Vino notify-on-connect false
gsettings set org.gnome.Vino icon-visibility 'never'
gsettings set org.gnome.Vino prompt-enabled false
gsettings set org.gnome.Vino require-encryption true
gsettings set org.gnome.Vino lock-screen-on-disconnect false